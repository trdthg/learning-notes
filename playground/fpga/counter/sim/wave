o0x7f8e8ef740a8 .functor BUFZ 1, C4<z>; HiZ drive
v0x55982e00a390_0 .net "rst", 0 0, o0x7f8e8ef740a8;  0 drivers
E_0x55982dfbf420 .event posedge, v0x55982dfbecd0_0;
S_0x55982dff7b60 .scope module, "tb" "tb" 3 2;
 .timescale -9 -12;
P_0x55982dff7cf0 .param/l "NCLK" 0 3 6, +C4<00000000000000000000000000000100>;
v0x55982e00a540_0 .var *"_ivl_0", 0 0; Local signal
v0x55982e00a640_0 .var "clk", 0 0;
v0x55982e00a700_0 .var "rst", 0 0;
E_0x55982dff70d0 .event posedge, v0x55982e00a640_0;
    .scope S_0x55982dff7960;
T_0 ;
    %wait E_0x55982dfbf420;
    %load/vec4 v0x55982e00a390_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55982e00a1e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55982e00a2c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55982e00a1e0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55982e00a1e0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55982dff7b60;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55982e00a640_0, 0, 1;
T_1.0 ;
    %load/vec4 v0x55982e00a640_0;
    %inv;
    %store/vec4 v0x55982e00a540_0, 0, 1;
    %pushi/vec4 2000, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x55982e00a540_0;
    %store/vec4 v0x55982e00a640_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x55982dff7b60;
T_2 ;
    %vpi_call 3 17 "$dumpfile", "wave.lxt2" {0 0 0};
    %vpi_call 3 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55982dff7b60 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55982dff7b60;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55982e00a700_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55982e00a700_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55982e00a700_0, 0, 1;
    %pushi/vec4 100, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55982dff70d0;
    %jmp T_3.0;
T_3.1 ;
