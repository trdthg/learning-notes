S_0x5643bda99330 .scope module, "foo" "foo" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /INPUT 1 "c";
    .port_info 3 /OUTPUT 1 "o";
o0x7f71ada3a048 .functor BUFZ 1, C4<z>; HiZ drive
o0x7f71ada3a078 .functor BUFZ 1, C4<z>; HiZ drive
L_0x5643bdaac380 .functor AND 1, o0x7f71ada3a048, o0x7f71ada3a078, C4<1>, C4<1>;
o0x7f71ada3a0a8 .functor BUFZ 1, C4<z>; HiZ drive
L_0x5643bdaac4a0 .functor OR 1, L_0x5643bdaac380, o0x7f71ada3a0a8, C4<0>, C4<0>;
v0x5643bda60cd0_0 .net *"_ivl_0", 0 0, L_0x5643bdaac380;  1 drivers
v0x5643bdaabd00_0 .net "a", 0 0, o0x7f71ada3a048;  0 drivers
v0x5643bdaabdc0_0 .net "b", 0 0, o0x7f71ada3a078;  0 drivers
v0x5643bdaabe90_0 .net "c", 0 0, o0x7f71ada3a0a8;  0 drivers
v0x5643bdaabf50_0 .net "o", 0 0, L_0x5643bdaac4a0;  1 drivers
S_0x5643bda99530 .scope module, "tb" "tb" 3 2;
 .timescale -9 -12;
P_0x5643bda996c0 .param/l "NCLK" 0 3 6, +C4<00000000000000000000000000000100>;
v0x5643bdaac120_0 .var *"_ivl_0", 0 0; Local signal
v0x5643bdaac220_0 .var "clk", 0 0;
v0x5643bdaac2e0_0 .var "rst", 0 0;
E_0x5643bda61d00 .event posedge, v0x5643bdaac220_0;
    .scope S_0x5643bda99530;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5643bdaac220_0, 0, 1;
T_0.0 ;
    %load/vec4 v0x5643bdaac220_0;
    %inv;
    %store/vec4 v0x5643bdaac120_0, 0, 1;
    %pushi/vec4 2000, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x5643bdaac120_0;
    %store/vec4 v0x5643bdaac220_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_0x5643bda99530;
T_1 ;
    %vpi_call 3 17 "$dumpfile", "wave.lxt2" {0 0 0};
    %vpi_call 3 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5643bda99530 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5643bda99530;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5643bdaac2e0_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5643bdaac2e0_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5643bdaac2e0_0, 0, 1;
    %pushi/vec4 100, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5643bda61d00;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call 3 29 "$display", "\350\277\220\350\241\214\347\273\223\346\235\237\357\274\201" {0 0 0};
    %vpi_call 3 30 "$dumpflush" {0 0 0};
    %vpi_call 3 31 "$finish" {0 0 0};
    %vpi_call 3 32 "$stop" {0 0 0};
    %end;
    .thread T_2;
